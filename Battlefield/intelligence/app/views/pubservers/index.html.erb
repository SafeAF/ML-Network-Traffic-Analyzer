<p id="notice"><%= notice %></p>

<h1>Listing Pubservers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Ip</th>
      <th>Hostname</th>
      <th>Os</th>
      <th>Dns</th>
      <th>Whois</th>
      <th>Organization</th>
      <th>Organization</th>
      <th>Reputation</th>
      <th>Url</th>
      <th>Applications</th>
      <th>Application</th>
      <th>Service</th>
      <th>App version</th>
      <th>Webserver</th>
      <th>Webserver</th>
      <th>Webserver version</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pubservers.each do |pubserver| %>
      <tr>
        <td><%= pubserver.name %></td>
        <td><%= pubserver.ip %></td>
        <td><%= pubserver.hostname %></td>
        <td><%= pubserver.os %></td>
        <td><%= pubserver.dns %></td>
        <td><%= pubserver.whois %></td>
        <td><%= pubserver.organization %></td>
        <td><%= pubserver.organization %></td>
        <td><%= pubserver.reputation %></td>
        <td><%= pubserver.url %></td>
        <td><%= pubserver.applications %></td>
        <td><%= pubserver.application %></td>
        <td><%= pubserver.service %></td>
        <td><%= pubserver.app_version %></td>
        <td><%= pubserver.webserver %></td>
        <td><%= pubserver.webserver %></td>
        <td><%= pubserver.webserver_version %></td>
        <td><%= link_to 'Show', pubserver %></td>
        <td><%= link_to 'Edit', edit_pubserver_path(pubserver) %></td>
        <td><%= link_to 'Destroy', pubserver, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pubserver', new_pubserver_path %>

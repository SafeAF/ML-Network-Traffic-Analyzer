<p id="notice"><%= notice %></p>

<h1>Listing Githubs</h1>

<table>
  <thead>
    <tr>
      <th>Owner</th>
      <th>Member</th>
      <th>Username</th>
      <th>Password</th>
      <th>Apikey</th>
      <th>User</th>
      <th>Url</th>
      <th>Membership</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @githubs.each do |github| %>
      <tr>
        <td><%= github.owner %></td>
        <td><%= github.member %></td>
        <td><%= github.username %></td>
        <td><%= github.password %></td>
        <td><%= github.apikey %></td>
        <td><%= github.user %></td>
        <td><%= github.url %></td>
        <td><%= github.membership %></td>
        <td><%= link_to 'Show', github %></td>
        <td><%= link_to 'Edit', edit_github_path(github) %></td>
        <td><%= link_to 'Destroy', github, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Github', new_github_path %>

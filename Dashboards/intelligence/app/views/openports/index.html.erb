<p id="notice"><%= notice %></p>

<h1>Listing Openports</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Desc</th>
      <th>Port</th>
      <th>Ip</th>
      <th>Service</th>
      <th>Network</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @openports.each do |openport| %>
      <tr>
        <td><%= openport.name %></td>
        <td><%= openport.desc %></td>
        <td><%= openport.port %></td>
        <td><%= openport.ip %></td>
        <td><%= openport.service %></td>
        <td><%= openport.network %></td>
        <td><%= link_to 'Show', openport %></td>
        <td><%= link_to 'Edit', edit_openport_path(openport) %></td>
        <td><%= link_to 'Destroy', openport, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Openport', new_openport_path %>

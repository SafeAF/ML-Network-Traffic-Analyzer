<p id="notice"><%= notice %></p>

<h1>Listing Networks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Purpose</th>
      <th>Type</th>
      <th>Speed</th>
      <th>Infrastructure</th>
      <th>Ownership</th>
      <th>Netadmin</th>
      <th>User</th>
      <th>Cluster</th>
      <th>Gateway ip</th>
      <th>Ping</th>
      <th>Hops</th>
      <th>Latency</th>
      <th>Router ip</th>
      <th>Broadcast</th>
      <th>Address space</th>
      <th>Dns</th>
      <th>Ptr record</th>
      <th>A record</th>
      <th>Reverse address</th>
      <th>Network box</th>
      <th>Operations</th>
      <th>Wifi ssid</th>
      <th>Wan ip</th>
      <th>Lan ip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @networks.each do |network| %>
      <tr>
        <td><%= network.name %></td>
        <td><%= network.purpose %></td>
        <td><%= network.type %></td>
        <td><%= network.speed %></td>
        <td><%= network.infrastructure %></td>
        <td><%= network.ownership %></td>
        <td><%= network.netadmin %></td>
        <td><%= network.user %></td>
        <td><%= network.cluster %></td>
        <td><%= network.gateway_ip %></td>
        <td><%= network.ping %></td>
        <td><%= network.hops %></td>
        <td><%= network.latency %></td>
        <td><%= network.router_ip %></td>
        <td><%= network.broadcast %></td>
        <td><%= network.address_space %></td>
        <td><%= network.dns %></td>
        <td><%= network.ptr_record %></td>
        <td><%= network.a_record %></td>
        <td><%= network.reverse_address %></td>
        <td><%= network.network_box %></td>
        <td><%= network.operations %></td>
        <td><%= network.wifi_ssid %></td>
        <td><%= network.wan_ip %></td>
        <td><%= network.lan_ip %></td>
        <td><%= link_to 'Show', network %></td>
        <td><%= link_to 'Edit', edit_network_path(network) %></td>
        <td><%= link_to 'Destroy', network, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Network', new_network_path %>

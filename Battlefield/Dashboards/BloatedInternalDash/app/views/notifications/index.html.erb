<p id="notice"><%= notice %></p>

<h1>Listing Notifications</h1>

<table>
  <thead>
    <tr>
      <th>Machine</th>
      <th>User</th>
      <th>Server</th>
      <th>Message</th>
      <th>Priority</th>
      <th>Source</th>
      <th>Destination</th>
      <th>Cluster</th>
      <th>Service</th>
      <th>Application</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notifications.each do |notification| %>
      <tr>
        <td><%= notification.machine %></td>
        <td><%= notification.user %></td>
        <td><%= notification.server %></td>
        <td><%= notification.message %></td>
        <td><%= notification.priority %></td>
        <td><%= notification.source %></td>
        <td><%= notification.destination %></td>
        <td><%= notification.cluster %></td>
        <td><%= notification.service %></td>
        <td><%= notification.application %></td>
        <td><%= link_to 'Show', notification %></td>
        <td><%= link_to 'Edit', edit_notification_path(notification) %></td>
        <td><%= link_to 'Destroy', notification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Notification', new_notification_path %>

<p id="notice"><%= notice %></p>

<h1>Listing Reputations</h1>

<table>
  <thead>
    <tr>
      <th>Ip</th>
      <th>Domain</th>
      <th>Confidence</th>
      <th>Description</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reputations.each do |reputation| %>
      <tr>
        <td><%= reputation.ip %></td>
        <td><%= reputation.domain %></td>
        <td><%= reputation.confidence %></td>
        <td><%= reputation.description %></td>
        <td><%= reputation.value %></td>
        <td><%= link_to 'Show', reputation %></td>
        <td><%= link_to 'Edit', edit_reputation_path(reputation) %></td>
        <td><%= link_to 'Destroy', reputation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reputation', new_reputation_path %>
